Algoritmo Ejercicio17
	
	Definir paginas, precioPorPagina, costoTotal Como Real
	
    // Datos fijos
    paginas <- 120
    precioPorPagina <- 150  // pesos por página
	
    // Calcular costo
    costoTotal <- paginas * precioPorPagina
	
    // Mostrar resultado
    Escribir "Número de páginas: ", paginas
    Escribir "Precio por página: $", precioPorPagina
    Escribir "Costo total de impresión: $", costoTotal

	
FinAlgoritmo
